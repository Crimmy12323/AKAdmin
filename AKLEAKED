-- Helper Functions for Player Dropdown
local function getPlayerNames()
    local names = {}
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer then
            table.insert(names, player.Name)
        end
    end
    return names
end

local function refreshDropdown(dropdown)
    if dropdown and dropdown.SetOptions then
        dropdown:SetOptions(getPlayerNames())
    end
end

-- Spy on Player Dropdown
local SelectedSpyPlayerName = ""

PlayerTab:CreateInput({
    Name = "Spy on Player",
    CurrentValue = "",
    PlaceholderText = "Enter Username or Display Name",
    RemoveTextAfterFocusLost = false,
    Flag = "SpyInput",
    Callback = function(Text)
        SelectedSpyPlayerName = Text
    end,
})

PlayerTab:CreateButton({
    Name = "Confirm Spy",
    Callback = function()
        if SelectedSpyPlayerName and SelectedSpyPlayerName ~= "" then
            for _, player in ipairs(Players:GetPlayers()) do
                if player.Name:lower():find(SelectedSpyPlayerName:lower()) or player.DisplayName:lower():find(SelectedSpyPlayerName:lower()) then
                    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                        Camera.CameraSubject = player.Character.HumanoidRootPart
                        Camera.CFrame = player.Character.HumanoidRootPart.CFrame + Vector3.new(0, 5, -10)
                        return
                    end
                end
            end
            warn("Player not found!")
        else
            warn("Please enter a valid username or display name!")
        end
    end,
})

PlayerTab:CreateButton({
    Name = "Stop Spying",
    Callback = function()
        Camera.CameraSubject = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
        Camera.CFrame = LocalPlayer.Character.HumanoidRootPart.CFrame + Vector3.new(0, 5, -10)
    end,
})
