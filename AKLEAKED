    Name = "Private Room Floor Collision",
    CurrentValue = false,
    Flag = "RoomFloor", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
    Callback = function(Value)
        for _, model in ipairs(workspace:GetChildren()) do
            if model:IsA("Model") and model.Name == "Room" then
                for _, child in ipairs(model:GetChildren()) do
                    if child:IsA("BasePart") and child.Name == "Part" then
                        child.CanCollide = Value
                    end
                end
            end
        end
    end,
