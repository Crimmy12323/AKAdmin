local Section = TeleportTab:CreateSection("Teleports")

TeleportTab:CreateButton({
    Name = "Teleport To Spawn",
    Callback = function()
        local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
        local rootPart = character:FindFirstChild("HumanoidRootPart")

        if not rootPart then
            return
        end

        local spawnPoints = {}
        for _, obj in ipairs(Workspace:GetChildren()) do
            if obj:IsA("Part") and obj.Name:lower():find("spawn") then
                table.insert(spawnPoints, obj)
            end
        end

        if #spawnPoints > 0 then
            local randomSpawn = spawnPoints[math.random(1, #spawnPoints)]
            rootPart.CFrame = randomSpawn.CFrame + Vector3.new(0, 5, 0)
        end
    end
})

local Button = TeleportTab:CreateButton({
    Name = "Teleport To Tower",
    Callback = function()
        local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
        local rootPart = character:FindFirstChild("HumanoidRootPart")

        if rootPart then
            rootPart.CFrame = CFrame.new(69.60, 284.56, 193.33)
        end
    end,
})

local function getPlayerNames()
    local names = {}
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer then
            table.insert(names, player.Name)
        end
    end
    return names
end

local function refreshDropdown(dropdown)
    if dropdown and type(dropdown.SetOptions) == "function" then
        dropdown:SetOptions(getPlayerNames())
    end
end

TeleportTab:CreateButton({
    Name = "Teleport To Booths",
    Callback = function()
        local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
        local rootPart = character:FindFirstChild("HumanoidRootPart")

        if rootPart then
            rootPart.CFrame = CFrame.new(136.39, 5.15, 60.84)
        end
    end,
})

TeleportTab:CreateButton({
    Name = "Teleport To Private Room",
    Callback = function()
        local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
        local rootPart = character:FindFirstChild("HumanoidRootPart")

        if not rootPart then
            return
        end

        for _, room in ipairs(Workspace:GetChildren()) do
            if room:IsA("Model") and room.Name == "Room" then
                local coffeeTable = room:FindFirstChild("houseInteriorCoffeeTable")
                if coffeeTable and coffeeTable:IsA("Model") then
                    local targetPart = coffeeTable:FindFirstChild("Part")
                    if targetPart and targetPart:IsA("BasePart") then
                        rootPart.CFrame = targetPart.CFrame + Vector3.new(0, 5, 0)
                        return
                    end
                end
            end
        end
    end
})
