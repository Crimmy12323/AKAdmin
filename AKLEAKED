                    [1] = "RAPESua",
                    [2] = "Gray",
                    [3] = "SourceSans"
                }
            }
        }

        -- Shuffle the remotes table
        for i = #remotes, 2, -1 do
            local j = math.random(1, i)
            remotes[i], remotes[j] = remotes[j], remotes[i]
        end

        -- Fire remotes without delay
        for _, remote in ipairs(remotes) do
            game:GetService("ReplicatedStorage"):WaitForChild("UpdateBoothText"):FireServer(unpack(remote.args))
        end
    end,
})
