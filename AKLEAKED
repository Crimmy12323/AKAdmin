Clearer2.Name = "Clearer2"
Clearer2.Parent = Holder
Clearer2.BackgroundColor3 = Color3.fromRGB(66, 66, 212)
Clearer2.Size = UDim2.new(0, 230, 0, 50)
Clearer2.Position = UDim2.new(0.5, -115, 0.3, 0)
