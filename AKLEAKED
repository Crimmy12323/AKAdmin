 -- Chat Log Start
 local Button = ChatTab:CreateButton({
    Name = "Anti Chat Logging",
    Callback = function()
        local randomstr = Instance.new("ScreenGui")
local Holder = Instance.new("Frame")
local UICorner = Instance.new("UICorner")
local Clearer = Instance.new("TextButton")
local ClearerUICorner = Instance.new("UICorner")
local Splitter = Instance.new("Frame")
local Info = Instance.new("TextLabel")
local MAX = Instance.new("UITextSizeConstraint")
local Clearer2 = Instance.new("TextButton")
local Clearer2UICorner = Instance.new("UICorner")
local Splitterrandomstr2 = Instance.new("Frame")
local UIGradient = Instance.new("UIGradient")

local Glow = Instance.new("ImageLabel")
Glow.Name = "Glow"
Glow.Parent = Holder
Glow.BackgroundTransparency = 1
Glow.Size = UDim2.new(1, 20, 1, 20) -- Slightly larger than the Holder
Glow.Position = UDim2.new(0, -10, 0, -10) -- Offset to create the glow effect
Glow.Image = "rbxassetid://5028857084" -- Glow effect asset
Glow.ImageColor3 = Color3.fromRGB(66, 66, 212) -- Blue color
Glow.ImageTransparency = 0.5 -- Adjust transparency for the glow intensity
Glow.ScaleType = Enum.ScaleType.Slice
Glow.SliceCenter = Rect.new(24, 24, 276, 276)

randomstr.Name = 'RobloxGui'
randomstr.Parent = game:GetService("CoreGui")
randomstr.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Holder.Name = "Holder"
Holder.Parent = randomstr
Holder.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
Holder.BackgroundTransparency = 0.2
Holder.BorderSizePixel = 0
Holder.Position = UDim2.new(0.35, 0, 0.3, 0)
Holder.Size = UDim2.new(0, 250, 0, 300)

UICorner.CornerRadius = UDim.new(0, 10)
UICorner.Parent = Holder

UIGradient.Color = ColorSequence.new{
    ColorSequenceKeypoint.new(0, Color3.fromRGB(66, 66, 212)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(45, 45, 45))
}
UIGradient.Parent = Holder

Clearer.Name = "Clearer"
Clearer.Parent = Holder
Clearer.BackgroundColor3 = Color3.fromRGB(66, 66, 212)
