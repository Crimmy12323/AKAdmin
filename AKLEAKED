    Name = "WalkSpeed Slider",
    Range = {0, 150},
    Increment = 1,
    Suffix = " Studs",
    CurrentValue = 16,
    Flag = "WalkSpeedSlider",
    Callback = function(Value)
        LocalPlayer.Character.Humanoid.WalkSpeed = Value
    end
})

-- Animation Speed Section
PlayerTab:CreateSection("Animation Speed")

-- Fast Animation Speed Keybind
PlayerTab:CreateKeybind({
    Name = "Fast Animation Speed",
    CurrentKeybind = "E",
    HoldToInteract = false,
    Flag = "FastAnimationKeybind",
    Callback = function()
        local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
        local humanoid = character:FindFirstChild("Humanoid")
        if humanoid then
            local animator = humanoid:FindFirstChildOfClass("Animator")
            if animator then
                for _, track in ipairs(animator:GetPlayingAnimationTracks()) do
                    track:AdjustSpeed(4)
                end
            end
        end
    end
})

-- Slow Animation Speed Keybind
PlayerTab:CreateKeybind({
    Name = "Slow Animation Speed",
    CurrentKeybind = "F",
    HoldToInteract = false,
    Flag = "SlowAnimationKeybind",
    Callback = function()
        local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
        local humanoid = character:FindFirstChild("Humanoid")
        if humanoid then
            local animator = humanoid:FindFirstChildOfClass("Animator")
            if animator then
                for _, track in ipairs(animator:GetPlayingAnimationTracks()) do
                    track:AdjustSpeed(0.1)
                end
            end
        end
    end
})

-- Rewind Animation Keybind
PlayerTab:CreateKeybind({
    Name = "Rewind Animation",
    CurrentKeybind = "C",
    HoldToInteract = false,
    Flag = "RewindAnimationKeybind",
    Callback = function()
        local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
        local humanoid = character:FindFirstChild("Humanoid")
        if humanoid then
            local animator = humanoid:FindFirstChildOfClass("Animator")
            if animator then
                for _, track in ipairs(animator:GetPlayingAnimationTracks()) do
                    track:AdjustSpeed(-0.5)
                end
            end
        end
    end
})

